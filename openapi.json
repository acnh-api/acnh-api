	"paths": {
		"/host-session/{dodo-code}": {
			"summary": "Get information about an active Dodo Code™.",
			"get": {
				"responses": {
					"200 OK": {
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Island-Session"
								}
							}
						},
						"description": "A summary of the island hosting session referenced by the given Dodo Code™.",
						"x-last-modified": 1591049169339
					},
					"404 Not Found": {
						"description": "In case the Dodo Code is not found.",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Dodo-Code-Error"
								}
							}
						},
						"x-last-modified": 1591049229615
					},
					"400 Bad Request": {
						"description": "In case the Dodo Code™ is invalid.",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Dodo-Code-Error"
								}
							}
						},
						"x-last-modified": 1591049263470
					}
				},
				"description": "",
				"tags": []
			},
			"x-last-modified": 1591046169736,
			"parameters": [
				{
					"name": "dodo-code",
					"in": "path",
					"example": "MQ530",
					"required": true,
					"deprecated": false,
					"description": "",
					"schema": {
						"example": "MQ530",
						"type": "string",
						"format": "",
						"pattern": "[ABCDEFGHJKLMNPQRSTUVWXY0-9]{5}",
						"enum": []
					},
					"x-last-modified": 1591048451480
				}
			]
		}
	},
	"components": {
		"securitySchemes": {},
		"schemas": {
			"Island-Session": {
				"example": {
					"active_players": 2,
					"name": "Bebopville",
					"host": "King",
					"start_time": "2020-06-04T02:12:16.744964+00:00"
				},
				"type": "object",
				"enum": [],
				"required": [
					"active_players",
					"name",
					"host",
					"start_time"
				],
				"properties": {
					"active_players": {
						"type": "integer",
						"format": "int32",
						"description": "How many players are currently on the island.",
						"minimum": 1,
						"maximum": 8
					},
					"name": {
						"type": "string",
						"description": "The name of the island.",
						"minLength": 1,
						"maxLength": 20
					},
					"host": {
						"type": "string",
						"description": "The name of the island's host.",
						"minLength": 1,
						"maxLength": 20
					},
					"start_time": {
						"type": "string",
						"format": "date-time",
						"description": "The time (in UTC) that the host started this session."
					}
				},
				"description": "A summary of the island hosting session.",
				"x-last-modified": 1591254860106
			},
			"Dodo-Code-Error": {
				"example": {
					"error": "invalid dodo code",
					"error_code": 2,
					"validation_regex": "[ABCDEFGHJKLMNPQRSTUVWXY0-9]{5}"
				},
				"type": "object",
				"enum": [],
				"required": [
					"error",
					"error_code"
				],
				"properties": {
					"error": {
						"type": "string",
						"description": "A human-readable description of the error."
					},
					"error_code": {
						"type": "integer",
						"format": "int32"
					},
					"validation_regex": {
						"type": "string",
						"format": "regex"
					}
				},
				"description": "An error occurred with the provided Dodo Code™.",
				"x-last-modified": 1591253713486
			}
		},
